{
  "name": "dingle-unleashed",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "check-types": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "babel": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "build": "yarn run check-types && yarn run babel",
    "export-vars": "heroku config -s >> .env"
  },
  "type": "commonjs",
  "repository": "git@github.com:danielwuhoo/dingle-unleashed.git",
  "author": "danielwuhoo <danielwuhoo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "@types/spotify-web-api-node": "^5.0.3",
    "@types/yt-search": "^2.3.2",
    "core-js": "^3.18.1",
    "discord-api-types": "^0.37.21",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "ffmpeg-static": "^4.4.0",
    "googleapis": "^87.0.0",
    "libsodium-wrappers": "^0.7.9",
    "mongoose": "^6.0.5",
    "node-cron": "^3.0.2",
    "node-fetch-commonjs": "^3.2.4",
    "parse-domain": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "spotify-web-api-node": "^5.0.2",
    "tsyringe": "^4.6.0",
    "typescript": "^4.4.3",
    "yt-search": "^2.10.1",
    "ytdl-core": "https://github.com/GreepTheSheep/node-ytdl-core.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "=16.x"
  }
}
